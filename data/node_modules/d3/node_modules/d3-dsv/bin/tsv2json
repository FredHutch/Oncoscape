#!/usr/bin/env node

var os = require("os"),
    rw = require("rw").dash,
    dsv = require("../");

rw.readFile(process.argv[2] || "-", "utf8", function(error, text) {
  if (error) throw error;
  rw.writeFile("-", JSON.stringify(dsv.tsvParse(text)) + os.EOL, "utf8", function(error) {
    if (error) throw error;
  });
});
